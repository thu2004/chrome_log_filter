{
  "manifest_version": 3,
  "name": "Log Filter",
  "version": "1.0",
  "description": "Filter log messages on webpages",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
